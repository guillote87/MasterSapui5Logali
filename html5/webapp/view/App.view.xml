<mvc:View controllerName="gq4dev.html5.controller.App" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Shell>
        <App>
            <pages>
                <Page title="{i18n>appTitle}">
                <headerContent>
                    <Button id="helloDialogHeader" icon="sap-icon://action" press="onOpenDialogHeader" class="sapUiSmallMarginEnd"/>
                </headerContent>
                    <content>
                        <mvc:XMLView viewName="gq4dev.html5.view.HelloPanel" />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
